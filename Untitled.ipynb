   "source": [
    "import pandas as pd\n",
    "from sklearn.preprocessing import MinMaxScaler\n",
    "\n",
    "# Загрузка данных\n",
    "df = pd.read_csv(r\"C:\\Users\\sudil\\Desktop\\уник\\ИИ\\1laba\\dataset\\train.csv\")\n",
    "\n",
    "# Проверка пропусков до заполнения\n",
    "print(\"Пропуски до заполнения:\")\n",
    "print(df.isna().sum())\n",
    "\n",
    "# Заполнение пропусков в числовых столбцах средним значением\n",
    "numeric_columns = [\"Age\", \"RoomService\", \"FoodCourt\", \"ShoppingMall\", \"Spa\", \"VRDeck\"]\n",
    "for column in numeric_columns:\n",
    "    df[column] = df[column].fillna(df[column].mean())\n",
    "\n",
    "# Нормализация числовых столбцов\n",
    "min_max_scaler = MinMaxScaler()\n",
    "df[numeric_columns] = min_max_scaler.fit_transform(df[numeric_columns])\n",
    "\n",
    "\n",
    "# Заполнение пропусков в категориальных столбцах модой\n",
    "categorical_columns = [\"HomePlanet\", \"Cabin\", \"Destination\"]\n",
    "for column in categorical_columns:\n",
    "    df[column] = df[column].fillna(df[column].mode()[0])\n",
    "df = pd.get_dummies(df, columns=categorical_columns, drop_first=True)\n",
    "\n",
    "# Заполнение пропусков в логических столбцах и приведение типов\n",
    "df[\"CryoSleep\"] = df[\"CryoSleep\"].fillna(False).astype(bool)\n",
    "df[\"VIP\"] = df[\"VIP\"].fillna(False).astype(bool)\n",
    "\n",
    "# Заполнение пропусков в столбце Name\n",
    "df[\"Name\"] = df[\"Name\"].fillna(\"Unknown\")\n",
    "\n",
    "# Проверка пропусков после заполнения\n",
    "print(\"\\nПропуски после заполнения:\")\n",
    "print(df.isna().sum())\n",
    "\n",
    "# Проверка, что все пропуски заполнены\n",
    "if df.isna().sum().sum() == 0:\n",
    "    print(\"Все пропуски заполнены.\")\n",
    "else:\n",
    "    print(\"Остались пропуски.\")\n",
    "\n",
    "# Просмотр первых строк\n",
    "print(\"\\nПервые строки данных:\")\n",
    "df.head()"
    "df.to_csv("processed_titanic.csv", index=False)"
   ]
